{
    "name": "idbi/key-manager",
    "description": "",
    "keywords": [
        "idbi",
        "key-manager"
    ],
    "homepage": "https://github.com/idbi/key-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "Nick Aguilar",
            "email": "nick.aguilar@idbi.pe",
            "homepage": "https://idbi.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "laravel/framework": "^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "symfony/var-dumper": "^5.0",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "ID\\KeyManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ID\\KeyManager\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ID\\KeyManager\\KeyManagerServiceProvider"
            ],
            "aliases": {
                "ID": "ID\\KeyManager\\KeyManagerFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
